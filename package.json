{
	"name": "baleadabot",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "@chikoshidori",
	"license": "MIT",
	"type": "module",
	"imports": {
		"#utils": "./dist/lib/utils.js",
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js",
		"#lib/env": "./dist/lib/env/index.js"
	},
	"scripts": {
		"build": "tsup",
		"start": "node --enable-source-maps dist/index.js",
		"dev": "run-s build start",
		"clean": "node scripts/clean.mjs",
		"watch": "tsup --watch",
		"watch:start": "tsup --onSuccess \"node ./dist/index.js\"",
		"lint": "eslint src --ext ts --fix",
		"format": "prettier --write \"src/**/*.ts\"",
		"commit": "git-cz",
		"update": "yarn up \"*\" -i",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"@discordjs/collection": "^0.5.0",
		"@sapphire/decorators": "^4.2.6",
		"@sapphire/discord-utilities": "^2.9.0",
		"@sapphire/discord.js-utilities": "^4.8.2",
		"@sapphire/fetch": "^2.2.0",
		"@sapphire/framework": "next",
		"@sapphire/plugin-i18next": "^2.2.3",
		"@sapphire/plugin-logger": "^2.1.3",
		"@sapphire/time-utilities": "^1.6.0",
		"@sapphire/type": "^2.1.2",
		"@sapphire/utilities": "^3.3.0",
		"discord.js": "^13.6.0",
		"dotenv-cra": "^3.0.2",
		"lexure": "^0.17.0",
		"reflect-metadata": "^0.1.13",
		"tsup": "^5.12.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^16.2.1",
		"@commitlint/config-conventional": "^16.2.1",
		"@sapphire/eslint-config": "^4.2.2",
		"@sapphire/plugin-hmr": "^1.1.1",
		"@sapphire/prettier-config": "^1.3.0",
		"@sapphire/ts-config": "^3.3.1",
		"@types/eslint": "^8",
		"@types/node": "^17.0.8",
		"@types/ws": "^8.5.3",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "8.11.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.5",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.5.1",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.3.2",
		"typescript": "^4.6.2"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire/eslint-config"
	},
	"packageManager": "yarn@3.2.0"
}

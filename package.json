{
	"name": "baleadabot",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "@chikoshidori",
	"license": "MIT",
	"type": "module",
	"private": true,
	"imports": {
		"#utils": "./dist/lib/utils.js",
		"#lib/*": "./dist/lib/*.js",
		"#root/*": "./dist/*.js"
	},
	"scripts": {
		"build": "tsup",
		"start": "node --enable-source-maps dist/index.js",
		"dev": "yarn build && yarn start",
		"clean": "node scripts/clean.mjs",
		"watch": "tsup --watch",
		"watch:start": "tsup --onSuccess \"node ./dist/index.js\"",
		"lint": "eslint --fix --ext ts src",
		"format": "prettier --write --loglevel=warn \"src/**/*.{js,ts,json}\"",
		"update": "yarn upgrade-interactive",
		"prepare": "husky install .github/husky"
	},
	"dependencies": {
		"@discordjs/builders": "1.2.0",
		"@discordjs/collection": "^1.1.0",
		"@sapphire/result": "^2.4.1",
		"@sapphire/utilities": "^3.9.3",
		"@skyra/env-utilities": "^1.0.2",
		"@skyra/http-framework": "^0.11.1",
		"@skyra/http-framework-i18n": "^0.3.5",
		"@skyra/safe-fetch": "^1.0.1",
		"@skyra/start-banner": "^1.0.2",
		"canvas-constructor": "^7.0.0",
		"colorette": "^2.0.19",
		"discord-api-types": "0.37.10",
		"gradient-string": "^2.0.2",
		"osu-api-extended": "2.1.5",
		"skia-canvas": "^1.0.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.4",
		"@sapphire/ts-config": "^3.3.4",
		"@types/gradient-string": "^1.1.2",
		"@types/node": "^18.7.18",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"pretty-quick": "^3.1.3",
		"standard-version": "^9.5.0",
		"tsup": "^6.2.3",
		"typescript": "^4.8.3"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"prettier": "@sapphire/prettier-config",
	"eslintConfig": {
		"extends": "@sapphire/eslint-config"
	},
	"packageManager": "yarn@3.2.0"
}
